---
import Page from "@layouts/Page.astro"
import Event from '@layouts/Home/Event'

import theme from "@config/theme";

const logo = import.meta.env.APP_LOGO;
const name = import.meta.env.APP_NAME;
const color = theme.light.primary;
---

<Page logo={logo} name={name} title="Event" description="Event Page" color={color}>
	<Event path="/event" client:only="solid" />
</Page>