---
import Page from "@layouts/Page.astro"
import Blog from '@layouts/Home/Blog'

import theme from "@config/theme";

const logo = import.meta.env.APP_LOGO;
const name = import.meta.env.APP_NAME;
const color = theme.light.primary;
---

<Page logo={logo} name={name} title="Blog" description="Blog Page" color={color}>
	<Blog path="/blog" client:only="solid" />
</Page>